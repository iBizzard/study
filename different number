#include <stdio.h>
#include <stdlib.h>
int cmpfunc(const void *a, const void *b)
{
	return ( *(int*)a-*(int*)b);
}

int main()
{
	int n,i,j,state=1;
	scanf("%d",&n);
	int a[n];
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	
		qsort(a,n,sizeof(int),cmpfunc);
	for(i=0;i<n-1;i++)
	{
		if(a[i]!=a[i+1])
		{
			state++;
		}
	}
	printf("%d\n",state);
	
}
